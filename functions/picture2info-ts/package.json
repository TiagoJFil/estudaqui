{
  "name": "picture2info-ts",
  "version": "1.0.0",
  "description": "TypeScript version of picture2info Firebase function",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "dotenv": "^16.1.4",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.3.2",
    "openai": "^5.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0"
  },
  "private": true
}

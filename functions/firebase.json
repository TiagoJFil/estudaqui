{
  "functions": [
    {
      "source": "pdf2img",
      "codebase": "pdf2img",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "runtime": "python311",
      "region": "us-west1",
      "environmentVariables": {
        "THREADS": "10"
      }
    },
    {
      "source": "picture2info-ts",
      "codebase": "picture2info-ts",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "runtime": "nodejs20",
      "region": "us-west1",
      "environmentVariables": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    }
  ]
}